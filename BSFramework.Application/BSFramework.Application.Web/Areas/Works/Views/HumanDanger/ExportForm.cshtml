@{
    ViewBag.Title = "ExportForm";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<script>
    function CheckUser() {
        dialogOpen({
            id: "UserForm",
            title: '选择分析人',
            url: "/../@Url.Action("UserForm")",
            width: "100%",
            height: "100%;",
            callBack: function (iframeId) {
                top.document.getElementById(iframeId).contentWindow.AcceptClick($("#trainingUser"), $("#trainingUserIds"));
                //
            }
        });
    }
    function AcceptClick() {
        if (!$('#form1').Validform()) {
            return false;
        }
        @*$.SaveForm({
            url: "/../@Url.Action("BatchPdf")" + "?trainingUserIds=" + $("#trainingUserIds").val() + "&start=" + $("#start").val() + "&end=" + $("#end").val(),
            type: "get",
            loading: "正在保存数据...",
            success: function () {
                
            }
        })*@
        window.location.href = "/../@Url.Action("BatchPdf")" + "?trainingUserIds=" + $("#trainingUserIds").val() + "&starttime=" + $("#start").val() + "&endtime=" + $("#end").val();
    }
</script>

<div style="margin-top: 20px; margin-right: 30px;" class="container">
    <table class="form">
        <tr>
            <td class="formTitle">选择分析人<font face="宋体" style="color:red;">*</font></td>
            <td class="formValue">
                <input id="trainingUser" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" readonly="readonly" onclick="CheckUser()" placeholder="点击选择"/>
                <input id="trainingUserIds" type="hidden" />
            </td>
        </tr>
        <tr>
            <td class="formTitle">选择日期</td>
            <td class="formValue col-md-12">
                <div class="form-group" >
                    <input type="text" class="form-control " style="width:45%;float:left;" id="start" onfocus="WdatePicker();" readonly="readonly" placeholder="开始时间">
                    <lable style="float:left; width:10%;text-align:center;">&nbsp;&nbsp;—&nbsp;&nbsp;</lable>
                     <input type="text" class="form-control" style="width:45%;float:left;" id="end" onfocus="WdatePicker();" readonly="readonly" placeholder="结束时间">
                </div>
            </td>
        </tr>
    </table>
</div>


